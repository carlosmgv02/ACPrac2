grep "CACHE" $amdDir/$tasca >> $dir
grep "sim_IPC" $amdDir/$tasca >> $dir

#GAP
cd /lib/specs2000/gap/data/ref
amdDir=$gap"/AMD"
tasca="AmdGAP.txt"
echo $tasca

sim-outorder -fastfwd 50000000 \
-max:inst 50000000 \
