-cache:dl1 CACHE_DL1:$CACHE_DL1 \
-cache:il2 CACHE_DL2:$CACHE_L2 \
-fetch:ifqsize $fetch \
-decode:width $decode \
-issue:width $issue \
-commit:width $commit \
-ruu:size $ruu \
-lsq:size $lsq \
-mem:lat $memlat \
-mem:width $memwidth \
-res:ialu $ialu \
-res:imult $imult \
-res:memport $memport \
-res:fpalu $fpalu \
-res:fpmult $fpmult \
-redir:sim $amdDir/$tasca /lib/specs2000/equake/exe/equake.exe < inp.in > $equake/inp.out 2> $equake/inp.err
    
